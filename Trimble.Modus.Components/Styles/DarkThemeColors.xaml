  <Color x:Key="WarningDark">#e49325</Color>

  <Color x:Key="Success">#1E8A44</Color>
  <Color x:Key="SuccessLight">#E0ECCF</Color>
  <Color x:Key="SuccessDark">#006638</Color>

  <Color x:Key="Secondary">#6A6E79</Color>
